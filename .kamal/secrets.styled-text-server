SECRETS=$(kamal secrets fetch --adapter doppler --from campsite-styled-text-server/prd KAMAL_REGISTRY_PASSWORD KAMAL_REGISTRY_USER_NAME API_BASE_URL AUTHTOKEN)

KAMAL_REGISTRY_USER_NAME=$(kamal secrets extract KAMAL_REGISTRY_USER_NAME $SECRETS)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD $SECRETS)
AUTHTOKEN=$(kamal secrets extract AUTHTOKEN $SECRETS)