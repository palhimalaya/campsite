{
  "name": "@campsite/sync-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch --onSuccess \"node dist/index.mjs\"",
    "sentry:sourcemaps": "sentry-cli --url https://glitchtip.allbrainy.com sourcemaps inject --org camp-site --project campsite-sync-server ./dist && sentry-cli --url https://glitchtip.allbrainy.com sourcemaps upload --org camp-site --project campsite-sync-server --log-level=debug ./dist",
    "start": "node dist/index.mjs"
  },
  "dependencies": {
    "@campsite/editor": "workspace:*",
    "@campsite/types": "workspace:*",
    "@hocuspocus/extension-database": "catalog:",
    "@hocuspocus/extension-logger": "catalog:",
    "@hocuspocus/server": "catalog:",
    "@hocuspocus/transformer": "catalog:",
    "@sentry/node": "catalog:",
    "@tiptap/core": "catalog:",
    "@tiptap/html": "catalog:",
    "@tiptap/pm": "catalog:",
    "dotenv": "catalog:",
    "js-base64": "catalog:",
    "yjs": "catalog:"
  },
  "devDependencies": {
    "@campsite/tsconfig": "workspace:*",
    "@flydotio/dockerfile": "catalog:",
    "@sentry/cli": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
